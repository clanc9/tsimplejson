<COMPONENT>
  <HEADER
    PLATEFORM="ALL"
    SERIAL=""
    VERSION="0.1"
    STATUS="ALPHA"
    COMPATIBILITY="ALL"
    AUTHOR="Christian Lanctot"
    RELEASE_DATE="20220301A"
    STARTABLE="false"
    LOG4J_ENABLED="true"
    DATA_AUTO_PROPAGATE="false"
    SCHEMA_AUTO_PROPAGATE ="true"
  >
    <SIGNATURE/>
  </HEADER>

  <FAMILIES>
    <FAMILY>JSON</FAMILY>
  </FAMILIES>

  <DOCUMENTATION>
  	<URL/>
  </DOCUMENTATION>

  <CONNECTORS>
    <CONNECTOR CTYPE="FLOW" MAX_INPUT="1" MAX_OUTPUT="1" MIN_OUTPUT="1"/>
    <CONNECTOR CTYPE="ITERATE" MAX_OUTPUT="0" MAX_INPUT="0"/>
    <CONNECTOR CTYPE="SUBJOB_OK" MAX_INPUT="1" />
    <CONNECTOR CTYPE="COMPONENT_OK"/>
    <CONNECTOR CTYPE="COMPONENT_ERROR"/>
    <CONNECTOR CTYPE="RUN_IF"/>
  </CONNECTORS>

  <PARAMETERS>

    <PARAMETER NAME="SCHEMA" FIELD="SCHEMA_TYPE" REQUIRED="true" NUM_ROW="20">
        <TABLE READONLY="false">
            <COLUMN NAME="simpleJson" TYPE="id_String" READONLY="true" CUSTOM="true" />
        </TABLE>
    </PARAMETER>

    <PARAMETER
      NAME="INCLUDE_ALL_COLUMNS"
      FIELD="CHECK"
      REQUIRED="true"
      NUM_ROW="30"
    >
      <DEFAULT>true</DEFAULT>
    </PARAMETER>

    <PARAMETER NAME="COLUMNS_TO_INCLUDE" FIELD="TABLE" REQUIRED="false"	NUM_ROW="40" NB_LINES="5" NOT_SHOW_IF="(INCLUDE_ALL_COLUMNS=='true')">
      <ITEMS>
        <ITEM NAME="INPUT_COLUMN" FIELD="PREV_COLUMN_LIST" REQUIRED="true" />
        <ITEM NAME="COL_CONVERT_TO_STRING" FIELD="CHECK" DEFAULT="false"/>
        <ITEM NAME="COL_INCLUDE_NULL" FIELD="CHECK" DEFAULT="false" />
        <ITEM NAME="COL_NULL_TO_EMPTY" FIELD="CHECK" DEFAULT="false" />
        <ITEM NAME="COL_CLEAN_STRING" FIELD="CHECK" DEFAULT="false" />
      </ITEMS>
    </PARAMETER>

    <PARAMETER
      NAME="CONVERT_TO_STRING"
      FIELD="CHECK"
      REQUIRED="false"
      NUM_ROW="50"
      NOT_SHOW_IF="(INCLUDE_ALL_COLUMNS=='false')"
    >
      <DEFAULT>false</DEFAULT>
  </PARAMETER>

  <PARAMETER
    NAME="INCLUDE_NULL"
    FIELD="CHECK"
    REQUIRED="false"
    NUM_ROW="60"
    NOT_SHOW_IF="(INCLUDE_ALL_COLUMNS=='false')"
  >
    <DEFAULT>true</DEFAULT>
  </PARAMETER>

    <PARAMETER
      NAME="NULL_TO_EMPTY"
      FIELD="CHECK"
      REQUIRED="false"
      NUM_ROW="65"
      NOT_SHOW_IF="(INCLUDE_ALL_COLUMNS=='false')"
    >
      <DEFAULT>false</DEFAULT>
    </PARAMETER>

    <PARAMETER
      NAME="CLEAN_STRING"
      FIELD="CHECK"
      REQUIRED="false"
      NUM_ROW="70"
      NOT_SHOW_IF="(INCLUDE_ALL_COLUMNS=='false')"
    >
      <DEFAULT>false</DEFAULT>
    </PARAMETER>
  </PARAMETERS>

	<ADVANCED_PARAMETERS>
      <PARAMETER
        NAME="DEFAULT_DATE_PATTERN"
        FIELD="TEXT"
        REQUIRED="false"
        NUM_ROW="10"
        >
        <DEFAULT>yyyy-MM-dd HH:mm:ss</DEFAULT>
    </PARAMETER>

    <PARAMETER
      NAME="DECIMAL_SEPARATOR_IS_COMMA"
      FIELD="CHECK"
      NUM_ROW="20"
      REQUIRED="false"
      >
      <DEFAULT>false</DEFAULT>
    </PARAMETER>


	</ADVANCED_PARAMETERS>


    <CODEGENERATION>
    <IMPORTS>
    </IMPORTS>
  </CODEGENERATION>

  <RETURNS>
    <RETURN NAME="NB_LINE" TYPE="id_Integer" AVAILABILITY="AFTER"/>
  </RETURNS>
</COMPONENT>
